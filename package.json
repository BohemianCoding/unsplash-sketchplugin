{
  "name": "unsplash",
  "version": "0.3.2",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "manifest": "src/manifest.json",
    "main": "unsplash.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.8"
  },
  "author": "Bohemian Coding",
  "dependencies": {
    "sketch-polyfill-fetch": "^0.3.3"
  }
}
